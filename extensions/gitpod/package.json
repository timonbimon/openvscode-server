{
	"name": "gitpod-desktop",
	"displayName": "%displayName%",
	"description": "%description%",
	"publisher": "gitpod",
	"version": "0.0.25",
	"license": "MIT",
	"preview": true,
	"icon": "resources/gitpod.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gitpod-io/vscode.git"
	},
	"bugs": {
		"url": "https://github.com/gitpod-io/gitpod/issues"
	},
	"engines": {
		"vscode": "^1.58.2"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"remote development",
		"remote"
	],
	"activationEvents": [
		"*",
		"onCommand:gitpod.api.autoTunnel",
		"onCommand:gitpod.api.auth",
		"onAuthenticationRequest:gitpod"
	],
	"contributes": {
		"commands": [
			{
				"command": "gitpod.api.auth",
				"title": "Authenthicate with GitPod"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"compile": "gulp compile-extension:gitpod",
		"watch": "gulp watch-extension:gitpod",
		"vscode:prepublish": "npm run compile"
	},
	"devDependencies": {
		"@types/node": "^10.12.21",
		"@types/node-fetch": "^2.5.12",
		"@types/tmp": "^0.2.1"
	},
	"dependencies": {
		"@gitpod/local-app-api-grpcweb": "main",
		"@improbable-eng/grpc-web-node-http-transport": "^0.14.0",
		"client-oauth2": "^4.3.3",
		"node-fetch": "^2.6.1",
		"tmp": "^0.2.1",
		"vscode-nls": "^5.0.0"
	},
	"extensionPack": [
		"ms-vscode-remote.remote-ssh"
	]
}
